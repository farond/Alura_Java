
public class TesteConexao {
	public static void main(String[] args) {
		
		try (Conexao conexao = new Conexao()){
			
		}
		
		
		
		//--------------------------
		
		Conexao con = null;		
		try {
			con = new Conexao();
			con.leDados();
			con.close();
		}
		catch(IllegalStateException ex){
			System.out.println("Erro de conexão");
			
		}
		finally {
			con.close();
		}
	}

}
